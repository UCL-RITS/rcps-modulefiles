#%Module -*- tcl -*-
#
# SWIG 3.0.5
#
#                                                      H.K. 15th June 2015

proc ModulesHelp { } {

    puts stderr "This module adds the SWIG 3.0.5 package to your environment."
    puts stderr "SWIG is an interface compiler that connects programs written in C"
    puts stderr "and C++ with scripting languages such as Perl, Python, Ruby, and Tcl."
    puts stderr ""
}

module-whatis "This module adds the SWIG 3.0.5 package to your environment. SWIG is an interface compiler that connects programs written in C and C++ with scripting languages such as Perl, Python, Ruby, and Tcl."

prereq gcc-libs/4.9.2
prereq compilers/gnu/4.9.2

conflict swig

set root 			/shared/ucl/apps/swig/3.0.5/gnu-4.9.2

prepend-path PATH      		$root/bin/

prepend-path CMAKE_PREFIX_PATH  $root
