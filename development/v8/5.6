proc ModulesHelp { } {

  puts stderr "\tAdds v8 to your environment variables"
}

module-whatis "adds v8 to your environment variables"

conflict v8
prereq gcc-libs/4.9.2

set              root              /shared/ucl/apps/v8/5.6/v8

prepend-path     PATH              $root/out.gn/ucl_rcas
prepend-path     LD_RUN_PATH       $root/out.gn/ucl_rcas
prepend-path     LD_LIBRARY_PATH   $root/out.gn/ucl_rcas

prepend-path     CPATH             $root/include
prepend-path     INCLUDE_PATH      $root/include

