proc ModulesHelp { } {

    puts stderr "This module adds Economics Department modules to your environment."
    puts stderr ""
    puts stderr "Allows access to applications licensed only for Economics users."
    puts stderr ""
}

module-whatis "Adds Economics Department modules to your environment."

module           use --append /shared/ucl/depts/economics/modulefiles
