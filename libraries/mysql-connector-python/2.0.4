#%Module -*- tcl -*-
#
# mysql-connector-python 2.0.4 
#
#                                                      

proc ModulesHelp { } {

    puts stderr "This module adds mysql-connector-python 2.0.4 to your environment."
    puts stderr ""

}

module-whatis "Adds mysql-connector-python 2.0.4 to your environment. This is Oracle's python-only MySQL connector for Python3"

prereq gcc-libs/4.9.2
prereq python3/3.5

conflict mysql-connector-python

set root 			/shared/ucl/apps/mysql-connector-python/2.0.4/gnu-4.9.2
prepend-path PYTHONPATH         $root/lib/python3.5/site-packages

